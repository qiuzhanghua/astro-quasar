---
// Component Imports
import Button from "../components/Button.astro";
import VueButton from "../components/VueButton.vue";
import QuasarButtons from "../components/QuasarButtons.vue";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Astro + Quasar</title>
  </head>

  <body>
    <div class="grid place-items-center h-screen content-center">
      <VueButton client:only="vue">Button of VueJS!</VueButton>
      <Button>Tailwind Button in Astro!</Button>
      <a href="/markdown-page" class="p-4 underline"
        >Markdown is also supported...</a
      >
      <QuasarButtons client:only="vue" />
    </div>
  </body>
</html>
